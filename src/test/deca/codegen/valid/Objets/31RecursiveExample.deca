class RecursionExample3 {  


    int factorial(int n){      
          if (n == 1)
          {      
            return 1; 
          }     
          else
          {      
            return (n * this.factorial(n-1));
          }
    }
}

{ 
    RecursionExample3 a = new RecursionExample3(); 
    println("Factorial of 5 is: ", a.factorial(5));  
}
